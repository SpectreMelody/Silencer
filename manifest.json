{
  "name":"Silencer",
  "version":"1.3",
  "description":"Alert Disabled",
  "manifest_version":2,
  "content_scripts":[
      {
          "matches":[
              "<all_urls>"
          ],
          "match_about_blank":true,
          "js":[
              "content.js"
          ],
          "run_at":"document_start",
          "all_frames":true
      }
  ],
  "permissions":[
      "tabs",
      "storage"
  ]
}